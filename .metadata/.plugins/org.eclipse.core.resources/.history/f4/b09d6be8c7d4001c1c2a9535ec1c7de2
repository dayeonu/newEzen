public class Generics {
	public static void main(String[] args) {

		Box aBox = new Box();
		Box oBox = new Box();
		
		aBox.setOb(new Apple());
		oBox.setOb(new Orange());
	}
}

class Box{
	private Object ob;

	public Object getOb() {
		return ob;
	}

	public void setOb(Object ob) {
		this.ob = ob;
	}
	
}

class Apple{
	@Override
	public String toString() {
		return "apple";
	}
}
class Orange{
	@Override
	public String toString() {
		return "orange";
	}
}

